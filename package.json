{
  "name": "geeky-chef",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently \"cd backend && nodemon\" \"cd frontend && npm start\"",
    "prepare": "husky",
    "test": "cd backend && npm test",
    "test:security": "snyk test --all-projects"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "snyk": "^1.1296.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache",
      "prettier --write"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "readable-stream": "^4.7.0"
  }
}
